{
  "name": "UploadTool",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=icon.png --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"UploadTool1\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^10.0.0"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "electron-packager": "^15.1.0",
    "electron-root-path": "^1.0.16",
    "fs": "0.0.1-security",
    "get-video-duration": "^3.0.2",
    "node-ssh": "^11.0.0",
    "sftp-client-promise": "^1.0.4",
    "ssh2": "^0.8.9",
    "ssh2-promise": "^0.1.7",
    "ssh2-sftp-client": "^5.3.0"
  }
}
